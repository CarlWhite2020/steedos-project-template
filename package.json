{
  "name": "my-steedos-project",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "services/*"
  ],
  "scripts": {
    "docker": "docker-compose up",
    "start": "moleculer-runner services/*/package.service.js --hot --repl",
    "repl": "moleculer-runner --repl",
    "nodered": "node-red --settings nodered.config.js",
    "db": "docker-compose -f docker-compose-db.yml up"
  },
  "dependencies": {
    "@steedos/service-package-loader": "^2.4.10",
    "@steedos/node-red-contrib-steedos": "~2.0.2",
    "dotenv-flow": "^3.2.0",
    "moleculer-repl": "^0.7.3",
    "node-red": "^3.0.2"
  }
}
